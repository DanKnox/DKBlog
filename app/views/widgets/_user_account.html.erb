<div class="user-account">
<% if signed_in? %>
	Signed in as: <%= current_user.name %><br><br>
	Not you? <%= link_to 'Sign Out', destroy_user_session_path, :method => :delete %>
<% else %>
	<%= link_to 'Sign in with Github', user_omniauth_authorize_path(:github) %><br><br>
	<%= link_to 'Sign in with Developer', user_omniauth_authorize_path(:developer) %>
<% end %>
</div>